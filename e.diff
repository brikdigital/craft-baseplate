+{#
+{% set url = url is defined ? url : null %}
+{% set type = type is defined ? type : 'link' %}
+{% set theme = theme is defined ? theme : null %}
+{% set fullWidth = fullWidth is defined ? fullWidth : false %}
+{% set fullWidthMobile = fullWidthMobile is defined ? fullWidthMobile : false %}
+{% set buttonClasses = buttonClasses is defined ? buttonClasses : false %}
+{% set icon = icon is defined ? icon : false %}
+{% set iconClasses = '' %}
+{% set rel = rel is defined ? rel : false %}
+
+{% set buttonStyling = 'inline-flex items-center justify-center font-bold text-base transition ease-in-out duration-150 '~styleClass %}
+{% if fullWidth %}
+	{% set buttonStyling = buttonStyling~' w-full' %}
+{% elseif fullWidth %}
+    {% set buttonStyling = buttonStyling~' w-full md:w-auto' %}
+{% endif %}
+
+{% set iconReplace = {
+    '--cart--' : '<svg class="inline-block w-5 h-5 mr-1" viewBox="0 0 24 24"><g stroke="currentColor" stroke-width="2" fill="none" fill-rule="evenodd" stroke-linecap="round" stroke-linejoin="round"><path d="M.688.688h4.125v12.374h13.75l2.75-9.624H8.938"/><circle cx="5.5" cy="19.25" r="2.063"/><circle cx="17.875" cy="19.25" r="2.063"/></g></svg>',
+} %}
+
+{% if type == 'link' and url != null %}
+	<a href="{{ url }}" target="{{ target }}" class="button {{ buttonStyling }} {{buttonClasses}}" {{ rel ? ('rel="'~rel~'"')|raw : null }}>
+
+		{% if icon %}
+			{% set imageType = icon.filename|split('.')|last %}
+			{% if imageType == 'svg' %}
+				{{ svg(icon)|attr({class: 'mr-2 w-6 h-6 fill-current '~iconClasses}) }}
+			{% else %}
+				{% set size = 'default' %}
+				{% set sizes = '(min-width: 1440px) 100px, (min-width: 1024px) and (max-width: 1439px) 5vw, 10vw' %}
+				{% set icon = craft.imager.transformImage(icon, size) %}
+				{% include "_partials/interface/images.twig" with {image: icon, size: 'icon', sizes: '400px', imageClasses: '', containerClasses: 'w-6 mr-2'} only %}
+			{% endif %}
+		{% endif %}
+		{{ text|replace(iconReplace)|raw }}
+	</a>
+{% elseif type == 'submit' %}
+	<button class="{{ buttonStyling }} {{buttonClasses}}" type="submit">
+		{% if icon is defined and icon|length %}
+			{{icon|replace(iconReplace)|raw}}
+		{% endif %}
+		{{ text|replace(iconReplace)|raw }}
+	</button>
+{% else %}
+	<button class="{{ buttonStyling }} {{buttonClasses}}">
+		{% if icon is defined and icon|length %}
+			{{icon|replace(iconReplace)|raw}}
+		{% endif %}
+		{{ text|replace(iconReplace)|raw }}
+	</button>
+{% endif %} #}
+
+{% set htmlTag = htmlTag ?? 'button' %}
+{% set baseClasses = 'inline-flex items-center justify-center font-bold text-base transition ease-in-out duration-150' %}
+{% set text = text is defined and text|length ? text : 'Meer info'|t %}
+{% set target = target is defined and target|length ? target : '_self' %}
+{% set disabled = disabled is defined and disabled|length ? disabled : false %}
+{% set theme = theme is defined and theme|length ? theme : 'primary' %}
+{% set fullWidth = fullWidth is defined ? fullWidth : 'default' %}
+{% set rel = rel is defined ? rel : false %}
+{% set buttonClasses = buttonClasses is defined and buttonClasses|length ? buttonClasses : false %}
+{% set icon = icon is defined ? icon : false %}
+{% set iconPosition = iconPosition is defined ? iconPosition : 'left' %}
+
+{% switch theme %}
+	{% case 'primary' %}
+		{% if icon %}
+			{% set iconClasses = 'filter brightness-0 invert' %}
+		{% endif %}
+		{% set themeClasses = 'bg-primary text-white hover:text-white hover:bg-primary/[.65]' %}
+	{% case 'secondary' %}
+		{% if icon %}
+			{% set iconClasses = 'filter brightness-1' %}
+		{% endif %}
+		{% set themeClasses = 'bg-secondary text-white hover:opacity-60' %}
+	{% case 'textDark' %}
+		{% if icon %}
+			{% set iconClasses = 'filter brightness-1' %}
+		{% endif %}
+		{% set themeClasses = 'text-black underline hover:text-primary' %}
+	{% case 'textLight' %}
+		{% if icon %}
+			{% set iconClasses = 'filter brightness-1' %}
+		{% endif %}
+		{% set themeClasses = 'text-white underline hover:text-secondary' %}
+{% endswitch %}
+
+{% set attributes = {
+	class: [baseClasses|join(' '), themeClasses|join(' '),]
+} %}
+
+{% switch size %}
+	{% case 'large' %}
+        {% set fontSize = 'text-lg' %}
+		{% set padding = 'px-5 py-3' %}
+	{% case 'small' %}
+        {% set fontSize = 'text-base' %}
+		{% set padding = 'px-3 py-2' %}
+{% endswitch %}
+
+{% switch fullWidth %}
+	{% case "allways" %}
+		{% set attributes = attributes|merge({class: [attributes.class|join(' '), 'w-full']}) %}
+	{% case "mobile" %}
+		{% set attributes = attributes|merge({class: [attributes.class|join(' '), 'w-full', 'md:w-auto']}) %}
+{% endswitch %}
+
+{% if theme starts with 'text' %}
+	{% set attributes = attributes|merge({class: [attributes.class|join(' '), fontSize|join(' ')]}) %}
+{% else %}
+	{% set attributes = attributes|merge({class: [attributes.class|join(' '), fontSize|join(' '), padding|join(' ')]}) %}
+{% endif %}
+
+{% if buttonClasses %}
+	{% set attributes = attributes|merge({class: [attributes.class|join(' '), buttonClasses|join(' ')]}) %}
+{% endif %}
+
+{% if htmlTag == 'a' %}
+    {% if url != '' %}
+        {% set attributes = attributes|merge({href: url}) %}
+    {% endif %}
+
+    {% if target %}
+        {% set attributes = attributes|merge({target: target}) %}
+        {% if target == '_blank'%}
+            {% set attributes = attributes|merge({rel: 'noopener noreferrer'}) %}
+        {% endif %}
+    {% endif %}
+
+	{% if rel %}
+		{% set attributes = attributes|merge({rel: rel|join(' ')}) %}
+	{% endif %}
+{% else %}
+    {% if disabled == true %}
+        {% set attributes = attributes|merge({'aria-disabled': 'true' }) %}
+        {% set attributes = attributes|merge({tabindex: '-1' }) %}
+        {% set attributes = attributes|merge({class: [attributes.class|join(' '), 'cursor-not-allowed opacity-50']}) %}
+    {% endif %}
+
+    {# {% set attributes = attributes|merge({role: button}) %} #}
+{% endif %}
+
+<{{ htmlTag }} {{attr(attributes)}}>
+    {% apply spaceless %}
+		{% if icon and iconPosition == 'left' %}
+			{{ inlineSvg(icon, 'mr-2 w-6 h-6 fill-current', { sanitize: true, namespace: true }) }}
+		{% endif %}
+
+		{{ text }}
+
+		{% if icon and iconPosition == 'right' %}
+			{{ inlineSvg(icon, 'ml-2 w-6 h-6 fill-current', { sanitize: true, namespace: true }) }}
+		{% endif %}
+    {% endapply %}
+</{{ htmlTag }}>
\ No newline at end of file
